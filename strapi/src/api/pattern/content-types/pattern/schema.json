{
  "collectionName": "patterns",
  "info": {
    "singularName": "pattern",
    "pluralName": "patterns",
    "displayName": "Pattern",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "relatedPatterns": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::pattern.pattern"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "phases": {
      "type": "customField",
      "default": "[]",
      "options": [
        "start",
        "discover_explore",
        "transform",
        "create",
        "implement"
      ],
      "customField": "plugin::multi-select.multi-select"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "entrepreneur",
        "team",
        "stakeholders",
        "product",
        "sustainability",
        "time_space"
      ]
    },
    "exercise": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::exercise.exercise"
    },
    "survey": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::survey.survey"
    },
    "subcategory": {
      "type": "string"
    },
    "patternId": {
      "type": "string"
    },
    "methods": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::method.method",
      "mappedBy": "patterns"
    },
    "questions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::question.question",
      "mappedBy": "patterns"
    }
  },
  "kind": "collectionType"
}
