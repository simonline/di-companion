{
  "name": "strapi",
  "version": "0.1.0",
  "private": true,
  "description": "A Strapi application",
  "scripts": {
    "build": "strapi build && npm run copy-templates",
    "deploy": "strapi deploy",
    "develop": "strapi develop & npm run copy-templates:watch",
    "start": "strapi start",
    "strapi": "strapi",
    "copy-templates": "mkdir -p dist/src/extensions/email/templates && cp -r src/extensions/email/templates/* dist/src/extensions/email/templates/",
    "copy-templates:watch": "nodemon --watch src/extensions/email/templates -e html --exec 'npm run copy-templates'"
  },
  "dependencies": {
    "@strapi/plugin-cloud": "5.1.1",
    "@strapi/plugin-users-permissions": "5.1.1",
    "@strapi/provider-email-nodemailer": "5.11.2",
    "@strapi/strapi": "5.1.1",
    "strapi-plugin-multi-select": "2.1.0",
    "pg": "8.8.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.0.0",
    "styled-components": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "typescript": "^5",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0 <=22.x.x",
    "npm": ">=6.0.0"
  },
  "strapi": {
    "uuid": "3fcc3a22-eaf7-4748-b143-af83fea0d378"
  }
}